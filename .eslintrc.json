{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true
  },
  "plugins": ["prettier", "simple-import-sort"],
  "rules": {
    "prettier/prettier": "error",

    "sort-imports": "off",
    "import/order": "off",
    "simple-import-sort/sort": "error",

    "react/jsx-curly-newline": "off"
  }
}
